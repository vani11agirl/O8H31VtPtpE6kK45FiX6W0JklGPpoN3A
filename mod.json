			"default": 5.0,
			"min": 0.1,
			"max": 50.0,
			"control": {
				"slider": true,
				"slider-step": 0.1
			}
		},
		"trail-width": {
			"name": "Trail Width",
			"description": "Width of the trail.",
			"type": "float",
			"default": 10.0,
			"min": 0.1,
			"max": 30.0,
			"control": {
				"slider": true,
				"slider-step": 0.5
			}
		},
		"trail-opacity": {
			"name": "Trail Opacity",
			"description": "Opacity of the trail (0 = transparent, 255 = opaque).",
			"type": "int",
			"default": 255,
			"min": 0,
			"max": 255,
			"control": {
				"slider": true,
				"slider-step": 5
			}
		},
		"disable-blending": {
			"name": "Disable Blending (Experimental)",
			"description": "please test",
